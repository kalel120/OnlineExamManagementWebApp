<div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" id="js-modal-editOrg">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">×</span>
                </button>
                <h3 class="modal-title" id="myModalLabel">Edit Organization</h3>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_content">
                                <form class="form-horizontal" id="js-modal-editOrg-form">
                                    <div class="item form-group">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Name</label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input id="js-modal-editOrg-name" class="form-control col-md-7 col-xs-12" 
                                                   placeholder="Should Not Exceed 20 Characters" 
                                                   required="required" type="text" 
                                                   name="Name">
                                            <span class="field-validation-valid text-danger" 
                                                  data-valmsg-for="Name" 
                                                  data-valmsg-replace="true">
                                            </span>
                                        </div>
                                    </div>
                                    <div class="item form-group">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Code</label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input type="text" id="js-modal-editOrg-code" class="form-control col-md-7 col-xs-12"
                                                   required="required"
                                                   name="Code">
                                            <span class="field-validation-valid text-danger"
                                                  data-valmsg-for="Code"
                                                  data-valmsg-replace="true">
                                            </span>
                                        </div>
                                    </div>
                                    <div class="item form-group">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Address</label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input type="text" id="js-modal-editOrg-address" class="form-control col-md-7 col-xs-12"
                                                   placeholder="Should Not Exceed 20 Characters"
                                                   required="required"
                                                   name="Address">
                                            <span class="field-validation-valid text-danger"
                                                  data-valmsg-for="Address"
                                                  data-valmsg-replace="true">
                                            </span>
                                        </div>
                                    </div>
                                    <div class="item form-group">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Contact No.</label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input type="text" id="js-modal-editOrg-contact" class="form-control col-md-7 col-xs-12"
                                                   placeholder="Should Not Exceed 20 Characters"
                                                   required="required"
                                                   name="Contact">
                                            <span class="field-validation-valid text-danger"
                                                  data-valmsg-for="Contact"
                                                  data-valmsg-replace="true">
                                            </span>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-success" id="js-modal-editOrg-Submit">Save changes</button>
            </div>
        </div>
    </div>
</div>

